{{new-studio saveStudio="saveStudio" myUser=model.users}}
{{new-city saveCity="saveCity" myCity=model.cities}}
{{new-user saveUser="saveUser"}}
<div class="row">
  <div class="col-sm-3">
    {{display-user}}
    <h3>Users:</h3>
    <hr>
    {{#each model.users as |user|}}
      {{user-tile user=user }}
    {{/each}}
  </div>
  <div class="col-sm-3">
    {{display-city}}
    <h3>Cities:</h3>
    <hr>
    {{#each model.cities as |city|}}
      {{city-tile city=city }}
    {{/each}}
  </div>
  <div class="col-sm-3">
    <h3>Studios:</h3>
    <hr>
    {{#each model.studios as |studio|}}
      {{studio-tile studio=studio }}
    {{/each}}
  </div>
</div>
